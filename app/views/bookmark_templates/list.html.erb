<h1>
  Bookmark list
</h1>

<a href="/new_bookmark_form">
  Add a new bookmark
</a>

<hr>

<table>
  <tr>
    <th>
      ID
    </th>

    <th>
      Sublessee
    </th>

    <th>
      Listing
    </th>

    <th>
      Created at
    </th>

    <th>
      Updated at
    </th>

    <th>
    </th>
  </tr>

  <% @bookmarks.each do |bookmark| %>
  <tr>
    <td>
      <%= bookmark.id %>
    </td>

    <td>
      <%= bookmark.sublessee_id %>
    </td>

    <td>
      <%= bookmark.listing_id %>
    </td>

    <td>
      <%= time_ago_in_words(bookmark.created_at) %> ago
    </td>

    <td>
      <%= time_ago_in_words(bookmark.updated_at) %> ago
    </td>

    <td>
      <a href="/bookmarks/<%= bookmark.id %>">
        Show details
      </a>
    </td>
  </tr>
  <% end %>
</table>
